{
    "name": "Enka Extension",
    "short_name": "Enka",
    "description": "Calculate Crit Value of Genshin character",
    "version": "1.5",
    "manifest_version": 3,
    "icons": {
        "16": "images/favicon-0.png",
        "24": "images/favicon-1.png",
        "32": "images/favicon-2.png",
        "48": "images/favicon-3.png",
        "64": "images/favicon-4.png",
        "96": "images/favicon-5.png",
        "128": "images/favicon-6.png"
    },
    "background": {
        "service_worker": "worker.js"
    },
    "content_scripts": [
        {
            "js": [
                "index.js"
            ],
            "matches": [
                "https://enka.network/*"
            ],
            "run_at": "document_idle"
        }
    ],
    "host_permissions": [
        "https://akasha.cv/*"
    ],
    "permissions": [
        "tabs"
    ]
}